{
  "name": "interactivesolutions/honeycomb-url-shortener",
  "description": "HoneyComb CMS package responsable for url shortening",
  "keywords": [
    "honeycomb",
    "honey",
    "laravel",
    "url",
    "url-shortener",
    "shortener",
    "short",
    "shorturl"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mantas GelÄius",
      "email": "mantas.gelcius@gmail.com"
    },
    {
      "name": "Liutauras Kavaliauskas",
      "email": "liutauraskav@gmail.com"
    },
    {
      "name": "Jevgenij Dmitrijev",
      "email": "jd@interactivesolutions.lt"
    },
    {
      "name": "Vytautas Rimeikis",
      "email": "vytautas.rimeikis@gmail.com"
    }
  ],
  "repositories": [],
  "require": {
    "php": ">=7.1",
    "laravel/framework": "^5.5",
    "interactivesolutions/honeycomb-core": "^1.1.0"
  },
  "require-dev": {
    "brianium/paratest": "^1.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench-browser-kit": "^3.5",
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "interactivesolutions\\honeycomburlshortener\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "paratest -p4 --colors"
    ]
  },
  "config": {
    "secure-http": false,
    "platform": {
      "php": "7.1"
    }
  },
  "minimum-stability": "stable"
}

